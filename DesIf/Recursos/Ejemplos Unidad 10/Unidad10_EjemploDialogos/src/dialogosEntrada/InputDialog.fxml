<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="190.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dialogosEntrada.InputDialogController">
   <children>
      <Label layoutX="95.0" layoutY="32.0" text="Diálogos de texto" textFill="#3222e1">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <HBox layoutX="142.0" layoutY="92.0" prefHeight="40.0">
         <children>
            <Button fx:id="textButton" alignment="CENTER" mnemonicParsing="false" text="Texto" HBox.hgrow="ALWAYS" />
            <Button fx:id="choiceButton" mnemonicParsing="false" text="Elección" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Text fx:id="userInput" fill="#9e1111" layoutX="95.0" layoutY="162.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Selección del usuario..." textAlignment="CENTER" wrappingWidth="218.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
